{
  "name": "platform-os-documentation",
  "version": "2.1.0",
  "description": "Documentation for platformOS",
  "private": true,
  "repository": "mdyd-dev/nearme-documentation",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "postinstall": "node ./scripts/check-node-version.js",
    "start": "npx webpack-cli -w",
    "clean": "git clean -Xfq $npm_package_config_build_dir/*.{css,js}",
    "autogenerate": "time ./scripts/generate_all.sh",
    "build": "NODE_ENV=production npx webpack-cli",
    "deploy": "npm run clean && npm run build && marketplace-kit deploy -f",
    "test": "docker run -v $PWD/tests/e2e:/tests -v $PWD/.marketplace-kit:/.marketplace-kit -it testcafe/testcafe \"chromium:headless --no-sandbox\" /tests -e MP_URL=\"staging\"",
    "test-ci": "testcafe \"chromium:headless --no-sandbox\" tests/e2e",
    "test-local": "npx testcafe \"chrome:headless\" tests/e2e"
  },
  "config": {
    "build_dir": "marketplace_builder/assets"
  },
  "author": "platformOS",
  "homepage": "https://documentation.platform-os.com",
  "browserslist": [
    "last 2 major versions",
    "ie >= 11",
    "not dead"
  ],
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-object-assign": "7.2.0",
    "@babel/preset-env": "7.3.4",
    "@babel/register": "7.0.0",
    "autoprefixer": "9.4.9",
    "babel-loader": "8.0.5",
    "css-loader": "2.1.0",
    "css-mqpacker": "7.0.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "postcss": "7.0.14",
    "postcss-calc": "7.0.1",
    "postcss-csso": "3.0.0",
    "postcss-fixes": "2.0.1",
    "postcss-focus": "4.0.0",
    "postcss-font-display": "^0.1.2",
    "postcss-loader": "3.0.0",
    "postcss-pseudoelements": "5.0.0",
    "sass-loader": "7.1.0",
    "semver": "5.6.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3",
    "webpack-require-from": "1.5.0"
  },
  "dependencies": {
    "@sentry/browser": "4.6.4",
    "anchor-js": "4.2.0",
    "bootstrap": "4.3.1",
    "ejs": "2.6.1",
    "metismenujs": "1.0.3",
    "prismjs": "1.15.0",
    "turbolinks": "5.2.0"
  },
  "peerDependencies": {
    "testcafe": "0.23.0"
  }
}
